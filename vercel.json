{
    "builds": [
      { "src": "index.html", "use": "@vercel/static" },
      { "src": "static/**/*", "use": "@vercel/static" },
      { "src": "templates/**/*.html", "use": "@vercel/static" }
    ],
    "routes": [
      { "src": "/static/(.*)", "dest": "/static/$1" },
      { "src": "/templates/(.*)", "dest": "/templates/$1" },
      { "src": "/(.*)", "dest": "/index.html" }
    ]
  }
  